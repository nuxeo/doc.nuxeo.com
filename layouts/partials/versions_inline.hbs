{{#if url.versions}}
<div id="versions_inline" class="row expanded">
    <ul class="menu">
        <li class="version-label">Documentation versions <span class="vertical-bar"></span></li>
        {{#each url.versions}}
        {{#unless this.no_page}}
        {{#unless (condition this.label '===' 'none')}}
        <li><a href="{{this.url}}" class="{{#if this.is_current_version}}active{{/if}}">{{this.label}}</a></li>
        {{/unless}}
        {{/unless}}
        {{/each}}
    </ul>
</div>
{{/if}}
